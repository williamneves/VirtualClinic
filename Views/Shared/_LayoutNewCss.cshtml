<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
    <title>@ViewData["Title"] - VirtualClinic</title>
    <link rel="stylesheet" href="~/css/site.css"/>
    <link rel="stylesheet" href="~/css/node_modules/mdb-ui-kit/css/mdb.min.css"/>
    
    
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.8.1/font/bootstrap-icons.css">
</head>
<body>
<div id="page-container">
<header>
    <partial name="_MBDnavbar.cshtml"/>
</header>
<div class="container-md" id="content-wrap">
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
</div>

<footer class="border-top text-muted p-2 mt-3 bg-white" id="footer">
    <div class="container-md">
        &copy; 2022 - VirtualClinic
    </div>
</footer>
<script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js"></script>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/css/node_modules/mdb-ui-kit/js/mdb.min.js"></script>

<script src="~/js/site.js" asp-append-version="true"></script>
@await RenderSectionAsync("Scripts", required: false)
</div>
</body>
</html>