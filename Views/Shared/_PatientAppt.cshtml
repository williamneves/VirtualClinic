@model Patient


<tbody>
    @if (ViewBag.PatientAppt.Appointments.Count != 0)
    {
        <table class="table">
        <thead>
            <tr>
                <th scope="col">Date of Visit</th>
                <th scope="col"> with</th>
            </tr>
        </thead>
        <tbody> 
            @foreach (var i in ViewBag.PatientAppt.Appointments)
            {
                <tr>
                    <td>@i.DateTime.Date</td>
                    <td>Dr. @i.Provider.FirstName @i.Provider.LastName</td>
                </tr>
            }    
        </tbody>
        </table>
    } else{
        <p>You do not have any previous visits. </p>
    }

